
	
<!-- register -->
	<div id="register" class="row jumbotron align-items-center" style="height:580px; margin-top: -30px; background-color:#D8BCBC;">
			<div class="col-md-8">
				<p class="lead" style="font-size: 38px;">Come be a part of this service</p>
			</div>
			<div class="col-md-4">
				<div class="container">
					<form action="register" method="post">
						<div class="form-group row">
						
							<div class="col-sm-10">
								<input type="text" name="fnameregister" class="form-control" id="fnameregister" placeholder="Name" required>
							</div>
						</div>
						<div class="form-group row">
					
							<div class="col-sm-10">
								<input type="email" name="femailregister" class="form-control" id="femailregister" placeholder="Email" required>
							</div>
						</div>
						<div class="form-group row">
						
							<div class="col-sm-10">
								<input type="password" name="fpasswordregister" class="form-control" id="fpasswordregister" placeholder="Password" required>
							</div>
						</div>
						<div class="form-group row">
							<div class="offset-sm-2 col-sm-10">
								<button type="submit" name="fsubmitregister" id="fsubmitregister" class="btn btn-primary">Register</button>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>